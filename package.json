{
  "name": "neff-snippets",
  "publisher": "duckyb",
  "repository": {
    "url": "https://github.com/duckyb/neff-helper-extension"
  },
  "icon": "images/logo.png",
  "displayName": "NEFF Snippets",
  "description": "Useful snippets for working with Net7's Frontend Framework",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "html",
        "path": "./snippets/components.code-snippets"
      },
      {
        "path": "./snippets/icon-font.code-snippets"
      },
      {
        "language": "scss",
        "path": "./snippets/styles.code-snippets"
      }
    ]
  }
}
